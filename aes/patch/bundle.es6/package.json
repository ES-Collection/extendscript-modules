  },
  "homepage": "https://github.com/ExtendScript/extendscript-modules#readme",
  "devDependencies": {
    "build-node-venv": "^1.0.3",
    "minimist": "^1.2.0",
    "tap-es": "git+https://github.com/ExtendScript/tap-es.git",
    "@extendscript/patch.array.from": "^1.0.0",
    "@extendscript/patch.object.assign": "^1.0.0",
    "@extendscript/patch.number.isfinite": "^1.0.0",
    "extendscript-bundlr": "^0.3.1"
